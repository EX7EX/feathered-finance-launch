# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_BACKUP_BUCKET=your_backup_bucket_name

# Security
JWT_SECRET=your_jwt_secret
ENCRYPTION_KEY=your_encryption_key

# Monitoring
SENTRY_DSN=your_sentry_dsn
ELASTIC_APM_SERVER_URL=your_apm_server_url
ELASTIC_APM_SERVICE_NAME=feathered-finance

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Backup Configuration
BACKUP_RETENTION_DAYS=30
BACKUP_SCHEDULE_DATABASE="0 0 * * *"
BACKUP_SCHEDULE_APPLICATION="0 */6 * * *"

# Performance
CACHE_TTL=3600
MAX_CONNECTIONS=100
TIMEOUT_MS=30000

# Logging
LOG_LEVEL=info
LOG_FORMAT=json
LOG_FILE_PATH=/var/log/feathered-finance

# Feature Flags
ENABLE_MAINTENANCE_MODE=false
ENABLE_BETA_FEATURES=false
ENABLE_ANALYTICS=true

# API Configuration
API_VERSION=v1
API_TIMEOUT=30000
API_RETRY_ATTEMPTS=3

# Database Configuration
DB_POOL_SIZE=20
DB_IDLE_TIMEOUT=30000
DB_CONNECTION_TIMEOUT=10000

# Cache Configuration
REDIS_URL=your_redis_url
REDIS_PASSWORD=your_redis_password
REDIS_TLS_ENABLED=true

# Email Configuration
SMTP_HOST=your_smtp_host
SMTP_PORT=587
SMTP_USER=your_smtp_user
SMTP_PASSWORD=your_smtp_password
EMAIL_FROM=noreply@feathered.finance

# External Services
STRIPE_SECRET_KEY=your_stripe_secret_key
STRIPE_WEBHOOK_SECRET=your_stripe_webhook_secret
COINMARKETCAP_API_KEY=your_cmc_api_key

# Maintenance
MAINTENANCE_WINDOW_START=02:00
MAINTENANCE_WINDOW_END=04:00
MAINTENANCE_NOTIFICATION_DAYS=7 